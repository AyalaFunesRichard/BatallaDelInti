            //x = goX;
            //y = goY;
            speed = 0;
            sprite_index = sp_duende_normal;
            status = "posFleeing";
        }
        break;
    }
    //
    case "posFleeing":{
        timerCounter += 1;
        if (timerCounter == (2.5*30))
        {
            status = "normal";
            timerCounter = 0;
        }  
        break;
    }    
}

